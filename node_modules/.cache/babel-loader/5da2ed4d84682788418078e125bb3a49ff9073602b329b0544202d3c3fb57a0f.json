{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  setup() {\n    let newtitle = ref(\"\");\n    let todos = ref([]);\n    let hey = ref([\"hello\", \"world\"]);\n    let responseData = ref([]);\n    onMounted(() => {\n      axios.get(\"http://localhost:3000/todos\").then(response => {\n        responseData.value = response.data;\n        console.log(responseData.value);\n      }).catch(error => {\n        console.error(error);\n      });\n    });\n    function logout() {\n      console.log(newtitle.value);\n    }\n    function addTodo() {\n      axios.post(\"http://localhost:3000/todos\", {\n        content: newtitle.value\n      });\n      console.log(newtitle.value);\n    }\n    return {\n      newtitle,\n      logout,\n      addTodo,\n      todos,\n      hey,\n      responseData\n    };\n  }\n};","map":{"version":3,"mappings":"AA0BA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,KAAK,GAAG;IACN,IAAIC,QAAO,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACtB,IAAIK,KAAI,GAAIL,GAAG,CAAC,EAAE,CAAC;IACnB,IAAIM,GAAE,GAAIN,GAAG,CAAC,CAAC,OAAO,EAAE,OAAO,CAAC,CAAC;IACjC,IAAIO,YAAW,GAAIP,GAAG,CAAC,EAAE,CAAC;IAC1BC,SAAS,CAAC,MAAM;MACdC,KAAI,CACDM,GAAG,CAAC,6BAA6B,EACjCC,IAAI,CAAEC,QAAQ,IAAK;QAClBH,YAAY,CAACI,KAAI,GAAID,QAAQ,CAACE,IAAI;QAClCC,OAAO,CAACC,GAAG,CAACP,YAAY,CAACI,KAAK,CAAC;MACjC,CAAC,EACAI,KAAK,CAAEC,KAAK,IAAK;QAChBH,OAAO,CAACG,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,SAASC,MAAM,GAAG;MAChBJ,OAAO,CAACC,GAAG,CAACV,QAAQ,CAACO,KAAK,CAAC;IAC7B;IACA,SAASO,OAAO,GAAG;MACjBhB,KAAK,CAACiB,IAAI,CAAC,6BAA6B,EAAE;QACxCC,OAAO,EAAEhB,QAAQ,CAACO;MACpB,CAAC,CAAC;MACFE,OAAO,CAACC,GAAG,CAACV,QAAQ,CAACO,KAAK,CAAC;IAC7B;IAEA,OAAO;MACLP,QAAQ;MACRa,MAAM;MACNC,OAAO;MACPb,KAAK;MACLC,GAAG;MACHC;IACF,CAAC;EACH;AACF,CAAC","names":["ref","onMounted","axios","setup","newtitle","todos","hey","responseData","get","then","response","value","data","console","log","catch","error","logout","addTodo","post","content"],"sourceRoot":"","sources":["/home/prism/Documents/VueApp/src/components/todoAppVue3.vue"],"sourcesContent":["<template>\n  <div>\n    <input\n      class=\"todo-input\"\n      type=\"text\"\n      @keyup.enter=\"addTodo\"\n      placeholder=\"something\"\n      v-model=\"newtitle\"\n    />\n  </div>\n  <div class=\"todo-section\">\n    <div v-for=\"todo in responseData\" :key=\"todo\">\n      <form>\n        <input type=\"checkbox\" onchange=\"\">\n      </form>\n      <form>\n        <input class=\"edit-todo\" v-model=\"todo.content\" />\n      </form>\n    </div>\n  </div>\n  <div>\n    <p>{{ newtitle }}</p>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nimport axios from \"axios\";\n\nexport default {\n  setup() {\n    let newtitle = ref(\"\");\n    let todos = ref([]);\n    let hey = ref([\"hello\", \"world\"]);\n    let responseData = ref([]);\n    onMounted(() => {\n      axios\n        .get(\"http://localhost:3000/todos\")\n        .then((response) => {\n          responseData.value = response.data;\n          console.log(responseData.value);\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    });\n\n    function logout() {\n      console.log(newtitle.value);\n    }\n    function addTodo() {\n      axios.post(\"http://localhost:3000/todos\", {\n        content: newtitle.value,\n      });\n      console.log(newtitle.value);\n    }\n\n    return {\n      newtitle,\n      logout,\n      addTodo,\n      todos,\n      hey,\n      responseData,\n    };\n  },\n};\n</script>\n<style scoped>\n.todo-input {\n  margin-top: 10rem;\n  height: 3rem;\n  width: 20rem;\n}\n.todo-section {\n  display: flex;\n  flex-direction: column-reverse;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}