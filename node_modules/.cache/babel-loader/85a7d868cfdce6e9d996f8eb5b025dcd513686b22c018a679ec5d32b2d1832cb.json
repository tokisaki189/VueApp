{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  mounted() {\n    setInterval(() => {\n      axios.get(\"http://localhost:3000/notes\").then(response => {\n        /* console.log(response.data); */\n        this.responseData = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    }, 100);\n  },\n  name: \"note-section\",\n  data() {\n    return {\n      responseData: [],\n      newid: 0,\n      newtitle: \"\",\n      newcontent: \"\",\n      searchText: \"\"\n    };\n  },\n  methods: {\n    addNote() {\n      axios.post(\"http://localhost:3000/addnote\", {\n        title: this.newtitle,\n        content: this.newcontent\n      });\n    },\n    deleteNote(id) {\n      axios.delete(\"http://localhost:3000/deletenote/\" + id);\n      console;\n    }\n  },\n  computed: {\n    filteredNotes() {\n      return this.responseData.filter(note => {\n        return note.title.toLowerCase().includes(this.searchText.toLowerCase()) || note.content.toLowerCase().includes(this.searchText.toLowerCase());\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,OAAO,GAAG;IACRC,WAAW,CAAC,MAAM;MAChBF,KAAI,CACDG,GAAG,CAAC,6BAA6B,EACjCC,IAAI,CAAEC,QAAQ,IAAK;QAClB;QACA,IAAI,CAACC,YAAW,GAAID,QAAQ,CAACE,IAAI;MACnC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC,EAAE,GAAG,CAAC;EACT,CAAC;EACDE,IAAI,EAAE,cAAc;EACpBJ,IAAI,GAAG;IACL,OAAO;MACLD,YAAY,EAAE,EAAE;MAChBM,KAAK,EAAE,CAAC;MACRC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRjB,KAAK,CAACkB,IAAI,CAAC,+BAA+B,EAAE;QAC1CC,KAAK,EAAE,IAAI,CAACN,QAAQ;QACpBO,OAAO,EAAE,IAAI,CAACN;MAChB,CAAC,CAAC;IACJ,CAAC;IACDO,UAAU,CAACC,EAAE,EAAE;MACbtB,KAAK,CAACuB,MAAM,CAAC,mCAAkC,GAAID,EAAE,CAAC;MACtDZ,OAAM;IACR;EACF,CAAC;EACDc,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAO,IAAI,CAACnB,YAAY,CAACoB,MAAM,CAAEC,IAAI,IAAK;QACxC,OACEA,IAAI,CAACR,KAAK,CAACS,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACd,UAAU,CAACa,WAAW,EAAE,KAC/DD,IAAI,CAACP,OAAO,CAACQ,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACd,UAAU,CAACa,WAAW,EAAE;MAErE,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["axios","mounted","setInterval","get","then","response","responseData","data","catch","error","console","name","newid","newtitle","newcontent","searchText","methods","addNote","post","title","content","deleteNote","id","delete","computed","filteredNotes","filter","note","toLowerCase","includes"],"sourceRoot":"","sources":["/home/prism/Documents/todoapp/src/components/notes.vue"],"sourcesContent":["<template>\n  <div>\n    <p>Add here</p>\n    <form @submit.prevent=\"addNote\" class=\"form-section\">\n      <input placeholder=\"title\" type=\"text\" v-model=\"newtitle\" />\n      <textarea placeholder=\"content\" v-model=\"newcontent\"></textarea>\n      <button type=\"submit\" @click=\"resetForm\">Add</button>\n    </form>\n\n    <input\n      type=\"text\"\n      v-model=\"searchText\"\n      placeholder=\"Search\"\n      class=\"search-box\"\n    />\n    note\n    <div class=\"note-container\">\n      <div v-for=\"note in filteredNotes\" :key=\"note\" class=\"notes\">\n        <p>{{ note.id }}</p>\n        <p>{{ note.title }}</p>\n        <p>{{ note.content }}</p>\n        <button v-bind:id=\"note.id\" @click=\"deleteNote(note.id)\">Delete</button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  mounted() {\n    setInterval(() => {\n      axios\n        .get(\"http://localhost:3000/notes\")\n        .then((response) => {\n          /* console.log(response.data); */\n          this.responseData = response.data;\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    }, 100);\n  },\n  name: \"note-section\",\n  data() {\n    return {\n      responseData: [],\n      newid: 0,\n      newtitle: \"\",\n      newcontent: \"\",\n      searchText: \"\",\n    };\n  },\n  methods: {\n    addNote() {\n      axios.post(\"http://localhost:3000/addnote\", {\n        title: this.newtitle,\n        content: this.newcontent,\n      });\n    },\n    deleteNote(id) {\n      axios.delete(\"http://localhost:3000/deletenote/\" + id);\n      console\n    },\n  },\n  computed: {\n    filteredNotes() {\n      return this.responseData.filter((note) => {\n        return (\n          note.title.toLowerCase().includes(this.searchText.toLowerCase()) ||\n          note.content.toLowerCase().includes(this.searchText.toLowerCase())\n        );\n      });\n    },\n  },\n};\n</script>\n<style>\n.note-container {\n  display: flex;\n  gap: 2rem;\n  width: 80%;\n  margin: auto;\n  padding: auto;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n\n.notes {\n  background-color: rgb(90, 255, 142);\n  width: 10rem;\n  height: 12rem;\n}\n\n.form-section {\n  margin: auto;\n  padding: auto;\n  width: 20%;\n  margin-bottom: 1rem;\n  display: flex;\n  flex-direction: column;\n}\n\n.form-section * {\n  border-radius: 2rem;\n  border: none;\n  padding: 0.5rem;\n  margin-bottom: 0.5rem;\n  height: 2rem;\n}\n\n.search-box {\n  border-radius: 2rem;\n  border: none;\n  padding: 0.5rem;\n  margin-bottom: 1rem;\n  height: 1.5rem;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}