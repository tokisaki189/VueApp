{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  mounted() {\n    axios.get(\"http://localhost:3000/notes\").then(response => {\n      /* console.log(response.data); */\n      this.responseData = response.data;\n    }).catch(error => {\n      console.error(error);\n    });\n  },\n  name: \"note-section\",\n  data() {\n    return {\n      responseData: [],\n      newid: 0,\n      newtitle: \"\",\n      newcontent: \"\",\n      searchText: \"\",\n      display: false\n    };\n  },\n  methods: {\n    addNote() {\n      axios.post(\"http://localhost:3000/addnote\", {\n        title: this.newtitle,\n        content: this.newcontent\n      });\n    },\n    deleteNote(id) {\n      console.log(id);\n      axios.delete(\"http://localhost:3000/deletenote/\" + id);\n    },\n    editNote(id) {\n      console.log(\"edit\" + id);\n    }\n    /* EditNote() {\n      router.push(\"/Edit\");\n    }, */\n  },\n\n  computed: {\n    filteredNotes() {\n      return this.responseData.filter(note => {\n        return note.title.toLowerCase().includes(this.searchText.toLowerCase()) || note.content.toLowerCase().includes(this.searchText.toLowerCase());\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,OAAO,GAAG;IACRD,KAAI,CACDE,GAAG,CAAC,6BAA6B,EACjCC,IAAI,CAAEC,QAAQ,IAAK;MAClB;MACA,IAAI,CAACC,YAAW,GAAID,QAAQ,CAACE,IAAI;IACnC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;MAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;IACtB,CAAC,CAAC;EACN,CAAC;EACDE,IAAI,EAAE,cAAc;EACpBJ,IAAI,GAAG;IACL,OAAO;MACLD,YAAY,EAAE,EAAE;MAChBM,KAAK,EAAE,CAAC;MACRC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRjB,KAAK,CAACkB,IAAI,CAAC,+BAA+B,EAAE;QAC1CC,KAAK,EAAE,IAAI,CAACP,QAAQ;QACpBQ,OAAO,EAAE,IAAI,CAACP;MAChB,CAAC,CAAC;IACJ,CAAC;IACDQ,UAAU,CAACC,EAAE,EAAE;MACbb,OAAO,CAACc,GAAG,CAACD,EAAE,CAAC;MACftB,KAAK,CAACwB,MAAM,CAAC,mCAAkC,GAAIF,EAAE,CAAC;IACxD,CAAC;IACDG,QAAQ,CAACH,EAAE,EAAE;MA