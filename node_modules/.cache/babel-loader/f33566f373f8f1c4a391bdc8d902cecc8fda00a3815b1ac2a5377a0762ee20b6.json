{"ast":null,"code":"import notes from \"./components/notes.vue\";\nimport Typo from \"./components/Typo.vue\";\nimport BigComp from \"./components/BigComp.vue\";\n/* import FormSection from './components/FormSection.vue' */\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      tabs: [\"notes\", \"Typo\", \"BigComp\"],\n      selected: \"notes\",\n      activeTabIndex: null,\n      items: ['Item 1', 'Item 2', 'Item 3', 'Item 4', 'Item 5', 'Item 6', 'Item 7', 'Item 8', 'Item 9', 'Item 10'],\n      colors: ['#FFC0CB', '#FFA07A', '#FFD700', '#ADFF2F', '#7FFFD4', '#87CEFA', '#6A5ACD', '#FF69B4', '#FF6347', '#FF4500']\n    };\n  },\n  components: {\n    notes,\n    Typo,\n    BigComp\n    /* FormSection */\n  },\n\n  methods: {\n    getRandomColor() {\n      return this.colors[Math.floor(Math.random() * this.colors.length)];\n    },\n    getTabColor(index) {\n      if (index === this.activeTabIndex) {\n        return 'white';\n      } else {\n        return 'rgb(90, 255, 142)';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,OAAM,MAAO,0BAA0B;AAC9C;AACA,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,OAAO,EAAE,MAAM,EAAE,SAAS,CAAC;MAClCC,QAAQ,EAAE,OAAO;MACjBC,cAAc,EAAE,IAAI;MACnBC,KAAK,EAAE,CAAC,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,SAAS,CAAC;MAC7GC,MAAM,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS;IACvH,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVV,KAAK;IACLC,IAAI;IACJC;IACA;EACF,CAAC;;EACDS,OAAO,EAAE;IACPC,cAAc,GAAG;MACf,OAAO,IAAI,CAACH,MAAM,CAACI,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,MAAM,EAAC,GAAI,IAAI,CAACN,MAAM,CAACO,MAAM,CAAC,CAAC;IACpE,CAAC;IACAC,WAAW,CAACC,KAAK,EAAE;MAClB,IAAIA,KAAI,KAAM,IAAI,CAACX,cAAc,EAAE;QACjC,OAAO,OAAO;MAChB,OAAO;QACL,OAAO,mBAAmB;MAC5B;IACF;EACF;AAAC","names":["notes","Typo","BigComp","name","data","tabs","selected","activeTabIndex","items","colors","components","methods","getRandomColor","Math","floor","random","length","getTabColor","index"],"sourceRoot":"","sources":["/home/prism/Documents/todoapp/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"button-container\">\n      <button\n        class=\"tab-menu\"\n        v-for=\"(tab, index) in tabs\"\n        :key=\"tab\"\n        :style=\"{ backgroundColor: getTabColor(index) }\"\n        @click=\"(selected = tab), (activeTabIndex = index)\"\n      >\n        {{ tab }}\n      </button>\n    </div>\n    <div class=\"component-container\">\n      <KeepAlive>\n        <component :is=\"selected\"></component>\n      </KeepAlive>\n    </div>\n  </div>\n   <div>\n    <div v-for=\"(item, index) in items\" :key=\"index\" :style=\"{ backgroundColor: getRandomColor }\" class=\"color-box\">{{ item }}</div>\n  </div>\n</template>\n\n<script>\nimport notes from \"./components/notes.vue\";\nimport Typo from \"./components/Typo.vue\";\nimport BigComp from \"./components/BigComp.vue\";\n/* import FormSection from './components/FormSection.vue' */\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      tabs: [\"notes\", \"Typo\", \"BigComp\"],\n      selected: \"notes\",\n      activeTabIndex: null,\n       items: ['Item 1', 'Item 2', 'Item 3', 'Item 4', 'Item 5', 'Item 6', 'Item 7', 'Item 8', 'Item 9', 'Item 10'],\n      colors: ['#FFC0CB', '#FFA07A', '#FFD700', '#ADFF2F', '#7FFFD4', '#87CEFA', '#6A5ACD', '#FF69B4', '#FF6347', '#FF4500']\n    };\n  },\n  components: {\n    notes,\n    Typo,\n    BigComp,\n    /* FormSection */\n  },\n  methods: {\n    getRandomColor() {\n      return this.colors[Math.floor(Math.random() * this.colors.length)];\n    },\n     getTabColor(index) {\n      if (index === this.activeTabIndex) {\n        return 'white';\n      } else {\n        return 'rgb(90, 255, 142)';\n      }\n    }\n  }}\n</script>\n\n<style>\nbody {\n  background-color: #0f87ff;\n}\n* {\n  margin: 0;\n  padding: auto;\n}\n.color-box {\n  width: 100px;\n  height: 100px;\n  margin-bottom: 10px;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n.container {\n  display: flex;\n}\n.button-container {\n  padding-top: 2rem;\n  display: flex;\n  flex-direction: column;\n  height: 53.3rem;\n  background-color: rgb(90, 255, 142);\n  width: 180px;\n}\n.component-container {\n  width: 80%;\n  background-color: rgb(255, 255, 255);\n}\n.fade-enter-active,\n.fade-leave-active {\n  transition: opacity 0.5s ease-in;\n}\n.tab-menu {\n  font-size: larger;\n  height: 4rem;\n  border-radius: 1rem 0 0 1rem;\n}\n.fade-enter-from,\n.fade-leave-to {\n  opacity: 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}