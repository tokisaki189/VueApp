{\n        content: newtitle.value,\n      });\n      console.log(newtitle.value);\n    }\n    function deleteTodo(id) {\n      console.log(id);\n    }\n   \n    return {\n      newtitle,\n      logout,\n      addTodo,\n      todos,\n      responseData,\n      deleteTodo,\n      scrollToTop,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.todo-container {\n  background-color: rgb(240, 240, 240);\n  height: 49.5rem;\n  overflow: hidden;\n}\n.todo-input {\n  margin-top: 5rem;\n  height: 3rem;\n  width: 20rem;\n}\n.todo-section {\n  margin: auto;\n  padding: auto;\n  padding-top: 4rem;\n\n  margin-top: 2rem;\n  display: flex;\n  flex-direction: column-reverse;\n  width: 70%;\n  height: 35rem;\n  overflow: scroll;\n}\n*::-webkit-scrollbar {\n  display: none;\n}\n.todo-check-form {\n  height: 3rem;\n  background-color: rgb(0, 136, 255);\n}\n.todo-check {\n  height: 3rem;\n  width: 3rem;\n  border: 1px solid rgb(0, 219, 248);\n  background-color: aqua;\n}\n.each-todo {\n  display: flex;\n  margin: auto;\n  padding: auto;\n  transition: transform 0.2s ease-in-out;\n  width: 50rem;\n  height: 3.5rem;\n}\n.each-todo:hover {\n  transform: scale(1.05);\n}\n.todo-form-input {\n  height: 4rem;\n  flex: 1;\n}\n.todo-content-input {\n  padding-left: 1rem;\n  border: none;\n  height: 3rem;\n  width: 100%;\n}\n.delete-todo {\n  height: 3rem;\n  font-size: larger;\n  font-weight: bolder;\n  color: white;\n  background-color: rgb(0, 255, 179);\n  padding-left: 0.5rem;\n  padding-right: 0.5rem;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}