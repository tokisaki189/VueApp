{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  mounted() {\n    axios.get(\"http://localhost:3000/notes\").then(response => {\n      /* console.log(response.data); */\n      this.responseData = response.data;\n    }).catch(error => {\n      console.error(error);\n    });\n  },\n  name: \"note-section\",\n  data() {\n    return {\n      responseData: [],\n      newid: 0,\n      newtitle: \"\",\n      newcontent: \"\",\n      searchText: \"\",\n      display: false\n    };\n  },\n  methods: {\n    addNote() {\n      axios.post(\"http://localhost:3000/addnote\", {\n        title: this.newtitle,\n        content: this.newcontent\n      });\n    },\n    deleteNote(id) {\n      console.log(id);\n      axios.delete(\"http://localhost:3000/deletenote/\" + id);\n    },\n    editNote(id, title, content) {\n      console.log(id, title, content);\n      axios.put(\"http://localhost:3000/editnote/\" + id, {\n        title: title,\n        content: content\n      });\n    }\n    /* EditNote() {\n      router.push(\"/Edit\");\n    }, */\n  },\n\n  computed: {\n    filteredNotes() {\n      return this.responseData.filter(note => {\n        return note.title.toLowerCase().includes(this.searchText.toLowerCase()) || note.content.toLowerCase().includes(this.searchText.toLowerCase());\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,OAAO,GAAG;IACRD,KAAI,CACDE,GAAG,CAAC,6BAA6B,EACjCC,IAAI,CAAEC,QAAQ,IAAK;MAClB;MACA,IAAI,CAACC,YAAW,GAAID,QAAQ,CAACE,IAAI;IACnC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;MAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;IACtB,CAAC,CAAC;EACN,CAAC;EACDE,IAAI,EAAE,cAAc;EACpBJ,IAAI,GAAG;IACL,OAAO;MACLD,YAAY,EAAE,EAAE;MAChBM,KAAK,EAAE,CAAC;MACRC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRjB,KAAK,CAACkB,IAAI,CAAC,+BAA+B,EAAE;QAC1CC,KAAK,EAAE,IAAI,CAACP,QAAQ;QACpBQ,OAAO,EAAE,IAAI,CAACP;MAChB,CAAC,CAAC;IACJ,CAAC;IACDQ,UAAU,CAACC,EAAE,EAAE;MACbb,OAAO,CAACc,GAAG,CAACD,EAAE,CAAC;MACftB,KAAK,CAACwB,MAAM,CAAC,mCAAkC,GAAIF,EAAE,CAAC;IACxD,CAAC;IACDG,QAAQ,CAACH,EAAE,EAAEH,KAAK,EAAEC,OAAO,EAAE;MAC3BX,OAAO,CAACc,GAAG,CAACD,EAAE,EAAEH,KAAK,EAAEC,OAAO,CAAC;MAC/BpB,KAAK,CAAC0B,GAAG,CAAC,iCAAgC,GAAIJ,EAAE,EAAE;QAChDH,KAAK,EAAEA,KAAK;QACZC,OAAO,EAAEA;MACX,CAAC,CAAC;IACJ;IACA;;;EAGF,CAAC;;EACDO,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAO,IAAI,CAACvB,YAAY,CAACwB,MAAM,CAAEC,IAAI,IAAK;QACxC,OACEA,IAAI,CAACX,KAAK,CAACY,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAAClB,UAAU,CAACiB,WAAW,EAAE,KAC/DD,IAAI,CAACV,OAAO,CAACW,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAAClB,UAAU,CAACiB,WAAW,EAAE;MAErE,CAAC,CAAC;IACJ;EACF;AACF,CAAC","names":["axios","mounted","get","then","response","responseData","data","catch","error","console","name","newid","newtitle","newcontent","searchText","display","methods","addNote","post","title","content","deleteNote","id","log","delete","editNote","put","computed","filteredNotes","filter","note","toLowerCase","includes"],"sourceRoot":"","sources":["/home/prism/Documents/todoapp/src/components/notes.vue"],"sourcesContent":["<template>\n  <div>\n    <p>Add here</p>\n    <form @submit=\"addNote\" class=\"form-section\">\n      <input placeholder=\"title\" type=\"text\" v-model=\"newtitle\" />\n      <textarea placeholder=\"content\" v-model=\"newcontent\"></textarea>\n      <button type=\"submit\" @click=\"resetForm\">Add</button>\n    </form>\n\n    <input\n      type=\"text\"\n      v-model=\"searchText\"\n      placeholder=\"Search\"\n      class=\"search-box\"\n    />\n    <div class=\"note-container\">\n      <div\n        v-for=\"note in filteredNotes.slice().reverse()\"\n        :key=\"note\"\n        class=\"notes\"\n        :id=\"note.id\"\n      >\n        <form\n          class=\"edit-form\"\n          :id=\"note.id\"\n          @submit.prevent=\"editNote(note.id, note.title, note.content)\"\n        >\n          <input\n            class=\"edit-input\"\n            v-model=\"note.title\"\n            onsubmit=\"event.preventDefault(); \"\n          />\n          <textarea v-model=\"note.content\" class=\"edit-textarea\"></textarea>\n          <div class=\"button-section\">\n            <button\n              class=\"delete-button\"\n              v-bind:id=\"note.id\"\n              @click=\"deleteNote(note.id)\"\n              onclick=\"event.preventDefault();\"\n            >\n              Delete\n            </button>\n            <button class=\"edit-button\" type=\"submit\">Edit</button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  mounted() {\n    axios\n      .get(\"http://localhost:3000/notes\")\n      .then((response) => {\n        /* console.log(response.data); */\n        this.responseData = response.data;\n      })\n      .catch((error) => {\n        console.error(error);\n      });\n  },\n  name: \"note-section\",\n  data() {\n    return {\n      responseData: [],\n      newid: 0,\n      newtitle: \"\",\n      newcontent: \"\",\n      searchText: \"\",\n      display: false,\n    };\n  },\n  methods: {\n    addNote() {\n      axios.post(\"http://localhost:3000/addnote\", {\n        title: this.newtitle,\n        content: this.newcontent,\n      });\n    },\n    deleteNote(id) {\n      console.log(id);\n      axios.delete(\"http://localhost:3000/deletenote/\" + id);\n    },\n    editNote(id, title, content) {\n      console.log(id, title, content);\n      axios.put(\"http://localhost:3000/editnote/\" + id, {\n        title: title,\n        content: content,\n      });\n    },\n    /* EditNote() {\n      router.push(\"/Edit\");\n    }, */\n  },\n  computed: {\n    filteredNotes() {\n      return this.responseData.filter((note) => {\n        return (\n          note.title.toLowerCase().includes(this.searchText.toLowerCase()) ||\n          note.content.toLowerCase().includes(this.searchText.toLowerCase())\n        );\n      });\n    },\n  },\n};\n</script>\n<style>\n*{\n  margin: 0;\n  padding: 0;\n}\n.note-container {\n  padding-top: 3rem;\n  height: 35rem;\n  background-color: blueviolet;\n  display: flex;\n  gap: 2rem;\n  overflow: scroll;\n  width: 70%;\n  margin: auto;\n  padding: auto;\n  padding-right: 1rem;\n  padding-left: 1rem;\n  flex-wrap: wrap;\n  align-items: center;\n}\n.note-container::-webkit-scrollbar {\n  display: none;\n}\n.button-section {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n}\n.notes {\n  padding: 1rem;\n  background-color: rgb(90, 255, 142);\n  width: 15rem;\n  height: 15rem;\n  transition: transform 0.3s ease-in-out;\n}\n.notes:hover {\n  transform: scale(1.05);\n}\n.delete-button {\n  margin-top: 2rem;\n  display: flex;\n  justify-content: flex-end;\n}\n.edit-input {\n  width: 100%;\n  height: 4rem;\n  font-size: large;\n  box-sizing: border-box;\n  background-color: rgb(90, 255, 142);\n  border: none;\n}\n.edit-textarea {\n  width: 100%;\n  height: 7rem;\n  box-sizing: border-box;\n  background-color: rgb(90, 255, 142);\n  border: none;\n  resize: none;\n}\n.edit-textarea::-webkit-scrollbar {\n  display: none;\n}\nbutton {\n  border: none;\n  background: none;\n}\n.edit-button {\n  margin-top: 2rem;\n  display: flex;\n  justify-content: flex-end;\n}\n.form-section {\n  margin: auto;\n  padding: auto;\n  width: 20%;\n  margin-bottom: 1rem;\n  display: flex;\n  flex-direction: column;\n}\n.form-section * {\n  border-radius: 2rem;\n  border: none;\n  padding: 0.5rem;\n  margin-bottom: 0.5rem;\n  height: 2rem;\n}\ntextarea {\n  outline: none;\n  resize: none;\n}\ninput {\n  outline: none;\n}\n.search-box {\n  border-radius: 2rem;\n  padding: 0.5rem;\n  border: 1px solid rgb(90, 255, 142);\n  margin-bottom: 1rem;\n  height: 1.5rem;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}